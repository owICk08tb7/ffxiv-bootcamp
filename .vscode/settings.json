{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "eslint.packageManager": "yarn",
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "javascript.validate.enable": false,
  "files.associations": {
    "*.scss": "tailwindcss"
  },
  "css.validate": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true,
    "**/.cache": true
  }
}
